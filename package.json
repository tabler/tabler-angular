{
  "name": "@tabler/tabler-angular",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --app demo --port=4200 --host 0.0.0.0 --aot",
    "docs": "ng serve --app docs --port=4201 --host 0.0.0.0 --aot",
    "build": "npm run build:demo && npm run build:docs",
    "build:demo": "ng build --prod --app demo",
    "build:docs": "ng build --prod --app docs",
    "build:libs": "node .bin/build-libs",
    "link:libs": "node .bin/link-libs",
    "release": "standard-version --commit-all",
    "prepublish": "npm run link:libs && npm run build:libs",
    "publish": "node .bin/publish-libs",
    "postpublish": "git push --follow-tags origin master",
    "publish:docs": "angular-cli-ghpages --dir ./dist/apps/docs",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected apps",
    "affected:build": "./node_modules/.bin/nx affected build",
    "affected:e2e": "./node_modules/.bin/nx affected e2e",
    "format": "./node_modules/.bin/nx format write",
    "format:write": "./node_modules/.bin/nx format write",
    "format:check": "./node_modules/.bin/nx format check",
    "update": "./node_modules/.bin/nx update",
    "update:check": "./node_modules/.bin/nx update check",
    "update:skip": "./node_modules/.bin/nx update skip",
    "postinstall": "./node_modules/.bin/nx postinstall",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic"
  },
  "standard-version": {
    "scripts": {
      "postbump": "node .bin/release && git add ."
    }
  },
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/router": "^5.2.9",
    "@ngrx/effects": "5.2.0",
    "@ngrx/router-store": "5.2.0",
    "@ngrx/store": "5.2.0",
    "@ngrx/store-devtools": "5.2.0",
    "@nrwl/nx": "0.11.0",
    "@swimlane/ngx-charts": "^7.2.0",
    "angular-pipes": "^7.1.0",
    "bootstrap": "^4.0.0",
    "chart.js": "^2.7.2",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "ng2-charts": "^1.6.0",
    "ngrx-store-freeze": "^0.2.1",
    "ngx-bootstrap": "^2.0.3",
    "ngx-highlightjs": "^1.4.1",
    "rxjs": "^5.5.8",
    "simple-line-icons": "^2.4.1",
    "time-ago-pipe": "^1.3.2",
    "zone.js": "^0.8.25"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/language-service": "^5.2.9",
    "@nrwl/schematics": "^0.11.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~9.6.2",
    "angular-cli-ghpages": "^0.5.2",
    "codelyzer": "^4.2.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.0.0",
    "ng-packagr": "^2.4.1",
    "prettier": "1.11.1",
    "protractor": "~5.3.1",
    "standard-version": "^4.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.8.1"
  }
}
